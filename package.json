{
  "name": "cuizijin-trap",
  "version": "1.0.0",
  "description": "崔子瑾诱捕器 - 网站访问控制应用",
  "main": "dist/main/index.js",
  "homepage": "./",
  "author": "开发团队",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "webpack --config webpack.main.config.js --mode development --watch",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "build:renderer": "vite build",
    "build:prod": "npm run clean && npm run build && npm run generate:icons",
    "dist": "electron-builder --config electron-builder.config.js",
    "dist:mac": "electron-builder --config electron-builder.config.js --mac",
    "dist:win": "electron-builder --config electron-builder.config.js --win",
    "dist:linux": "electron-builder --config electron-builder.config.js --linux",
    "dist:all": "electron-builder --config electron-builder.config.js --mac --win --linux",
    "dist:publish": "electron-builder --config electron-builder.config.js --publish=always",
    "clean": "rimraf dist release",
    "generate:icons": "node scripts/generate-icons.js",
    "prepare:release": "npm run build:prod && npm run dist:all",
    "deploy": "node scripts/deploy.js",
    "deploy:mac": "node scripts/deploy.js mac",
    "deploy:win": "node scripts/deploy.js win",
    "deploy:linux": "node scripts/deploy.js linux",
    "deploy:all": "node scripts/deploy.js all",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:auth": "playwright test tests/auth.spec.ts",
    "test:dashboard": "playwright test tests/dashboard.spec.ts",
    "test:components": "playwright test tests/components.spec.ts",
    "test:blocking": "playwright test tests/website-blocking.spec.ts",
    "test:cross-browser": "playwright test tests/cross-browser.spec.ts",
    "test:report": "playwright show-report",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:electron": "playwright test --config=playwright-electron.config.ts",
    "test:electron:ui": "playwright test --config=playwright-electron.config.ts --ui",
    "test:electron:debug": "playwright test --config=playwright-electron.config.ts --debug",
    "test:electron:headed": "playwright test --config=playwright-electron.config.ts --headed",
    "electron": "electron .",
    "electron:dev": "NODE_ENV=development electron ."
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^8.7.0",
    "clsx": "^1.2.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "framer-motion": "^10.0.0",
    "http-proxy-middleware": "^2.0.6",
    "lucide-react": "^0.321.0",
    "node-forge": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/better-sqlite3": "^7.6.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/node-forge": "^1.3.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^13.0.0",
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^30.0.2",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "website-blocker",
    "parental-control"
  ]
}
